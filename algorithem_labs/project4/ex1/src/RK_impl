#ifndef RK_IMPL
#define RK_IMPL
#include<cstring>
using namespace std;
template<typename Char_Type>
void RK(Char_Type*& P,unsigned int m,Char_Type*& T,unsigned int n,int q,int d)
{
    int h = 1;
    for(int i = 0;i < m - 1;i += 1)
    {
        h = (h * d) % q;
    }
    int p = 0,t = 0;
    for(int i = 0;i < m;i++)
    {
        p = (d * p + P[i]) % q;
        t = (d * t + T[i]) % q;
    }
    for(int s = 0;s < n - m;s++)
    {
        if(t == p)
        {
            if(memcmp(P,T+s,m) == 0)
            {
                cout << "发现一次成功匹配！" << "此时s = " << s << '\n';
            }
        }
        t = ((d * (t - ((h * T[s]) % q))) + T[s+m]) % q;
    }
    return ;
}

#endif